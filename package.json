{
  "name": "tap-together",
  "version": "0.2.0",
  "build": 20,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "bump": "jaq -i '.build|=.+1' package.json; git add package.json; git commit -m 'bump build'",
    "eas": "pnpx eas-cli@latest",
    "build:dev:all": "pnpm run eas build --no-wait --non-interactive --platform all --profile development",
    "build:dev:ios": "pnpm run eas build --no-wait --non-interactive --platform ios --profile development",
    "devclient:ios": "pnpm run eas build:run -p ios",
    "devclient:android": "pnpm run eas build:run -p android",
    "build:prod:all": "pnpm run eas build --no-wait --non-interactive --platform all --profile production",
    "build:prod:ios": "pnpm run eas build --no-wait --non-interactive --platform ios --profile production",
    "submit:ios": "pnpm run eas submit --no-wait --non-interactive --platform ios --latest",
    "submit:android": "pnpm run eas submit --no-wait --non-interactive --platform android --latest",
    "doctor": "pnpx expo-doctor@latest",
    "doctor:fix": "pnpx expo@latest install --check",
    "prebuild": "pnpx expo@latest prebuild"
  },
  "dependencies": {
    "@alevy97/react-native-userdefaults": "^0.2.1",
    "@types/react": "~18.2.14",
    "expo": "49.0.21",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "^2.4.12",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "firebase": "^10.7.1",
    "jotai": "^2.6.0",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-mmkv": "^2.11.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-widget-extension": "^0.0.5",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "tailwindcss": "3.3.2"
  },
  "pnpm": {
    "patchedDependencies": {
      "react-native-widget-extension@0.0.5": "patches/react-native-widget-extension@0.0.5.patch"
    },
    "overrides": {
      "@babel/preset-env": "7.1.6",
      "metro": "~0.76.7"
    }
  }
}
