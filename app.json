{
	"expo": {
		"version": "0.1.0",
		"name": "tap-together",
		"slug": "tap-together",
		"scheme": "tap-together",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"bundleIdentifier": "is.pvin.tap-together",
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": [["expo-router", { "root": "./src/routes" }]],
		"experiments": {
			"typedRoutes": true
		}
	}
}
